      "source": [
        "# Mongo\n",
        "MONGO_URI  = \"USE YOU MONGO DB CREDENTIALS"\n",
        "MONGO_DB   = \"slack_bot\"          # change if different\n",
        "MONGO_COLL = \"messages\"     # change if different\n",
        "\n",
        "# Optional date filter (ISO strings). Set to None to pull all.\n",
        "DATE_START = None\n",
        "DATE_END   = None\n",
        "\n",
        "# Expanded keywords to maximize hits (brands, tools, slang, common misspellings)\n",
        "AI_KEYWORDS = [\n",
        "    r\"\\bai\\b\", \"artificial intelligence\", \"machine learning\", r\"\\bml\\b\", \"deep learning\",\n",
        "    \"gen ai\", \"genai\", \"gpt\", \"chatgpt\", \"llm\", \"large language model\",\n",
        "    \"automation\", \"automate\", \"robotics\", \"agent\", \"agents\", \"copilot\",\n",
        "    \"prompt\", \"prompting\", \"rag\", \"vector db\",\n",
        "    \"openai\", \"anthropic\", \"claude\", \"mistral\", \"llama\", \"llama3\",\n",
        "    \"gemini\", \"bard\", \"perplexity\", \"cursor\", \"codeium\", \"copilot\", \"tabnine\"\n",
        "]\n",
        "\n",
        "# Reference topics (your list)\n",
        "REFERENCE_TOPICS = [\n",
        "    \"AI in the workforce\", \"artificial intelligence and jobs\", \"impact of AI at work\",\n",
        "    \"AI replacing jobs\", \"AI improving productivity\", \"automation and employment\",\n",
        "    \"machine learning in business\", \"future of work with AI\", \"AI tools for professionals\",\n",
