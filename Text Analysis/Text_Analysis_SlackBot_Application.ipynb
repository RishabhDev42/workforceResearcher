      "source": [
        "from sklearn.feature_extraction.text import CountVectorizer\n",
        "import numpy as np\n",
        "\n",
        "def top_ngrams(texts, ngram_range=(2,3), top_k=20, min_df=1, max_df=0.95):\n",
        "    vect = CountVectorizer(stop_words=\"english\", ngram_range=ngram_range, min_df=min_df, max_df=max_df)\n",
        "    X = vect.fit_transform([t for t in texts if isinstance(t,str)])\n",
        "    if X.shape[0] == 0:\n",
        "        return pd.DataFrame(columns=[\"ngram\",\"count\"])\n",
        "    counts = np.asarray(X.sum(axis=0)).ravel()\n",
        "    vocab = np.array(vect.get_feature_names_out())\n",
        "    order = counts.argsort()[::-1][:top_k]\n",
        "    return pd.DataFrame({\"ngram\": vocab[order], \"count\": counts[order]})\n",
        "\n",
        "# Overall\n",
        "ngrams_all = top_ngrams(df_ai[\"clean_text\"].tolist(), ngram_range=(2,3), top_k=20)\n",
        "ngrams_all.to_csv(f\"{OUTPUT_DIR}/top_ngrams_all.csv\", index=False)\n",
        "\n",
        "# Per sentiment (optional but useful)\n",
        "dfs = []\n",
        "for lab in [\"positive\",\"neutral\",\"negative\"]:\n",
        "    df_lab = top_ngrams(df_ai[df_ai[\"sentiment_label\"]==lab][\"clean_text\"].tolist(), (2,3), 15)\n",
        "    df_lab.insert(0, \"sentiment\", lab)\n",
        "    dfs.append(df_lab)\n",
        "ngrams_by_sent = pd.concat(dfs, ignore_index=True) if dfs else pd.DataFrame(columns=[\"sentiment\",\"ngram\",\"count\"])\n",
        "ngrams_by_sent.to_csv(f\"{OUTPUT_DIR}/top_ngrams_by_sentiment.csv\", index=False)\n",
        "\n",
        "# Plot top 20 overall\n",
        "if not ngrams_all.empty:\n",
        "    plt.figure(figsize=(10,5))\n",
        "    plt.bar(ngrams_all[\"ngram\"], ngrams_all[\"count\"])\n",
        "    plt.xticks(rotation=45, ha=\"right\"); plt.title(\"Top Bigrams/Trigrams in AI Messages\")\n",
        "    plt.tight_layout(); plt.savefig(f\"{OUTPUT_DIR}/top_ngrams.png\"); plt.close()\n",
        "\n",
        "print(\"Saved n-gram CSVs/PNG to:\", OUTPUT_DIR)\n"
