      "source": [
        "# Row-level enriched table (for drill-down)\n",
        "cols = [\"team_id\",\"channel\",\"ts\",\"user\",\"text\",\"created_at\",\n",
        "        \"clean_text\",\"sentiment_score\",\"sentiment_label\",\"topic_id\",\"reaction_count\"]\n",
        "# Ensure only existing columns are selected\n",
        "cols_to_export = [col for col in cols if col in df_ai.columns]\n",
        "(df_ai[cols_to_export]).to_csv(\n",
        "    f\"{OUTPUT_DIR}/messages_enriched.csv\", index=False\n",
        ")\n",
        "\n",
        "# Keywords\n",
        "pd.DataFrame(kw_pairs, columns=[\"term\",\"tfidf_sum\"]).to_csv(\n",
        "    f\"{OUTPUT_DIR}/top_keywords.csv\", index=False\n",
        ")\n",
        "\n",
        "# Topics\n",
        "pd.DataFrame(\n",
        "    [{\"topic_id\": t[\"topic_id\"], \"terms\": \", \".join(t[\"terms\"])} for t in topics]\n",
        ").to_csv(f\"{OUTPUT_DIR}/topics.csv\", index=False)\n",
        "\n",
        "# Channel summary KPIs\n",
        "def channel_summary(df_all, df_ai_enriched):\n",
        "    if \"channel\" not in df_all.columns or \"channel\" not in df_ai_enriched.columns:\n",
        "        print(\"Warning: 'channel' column not found in one or both dataframes. Skipping channel summary.\")\n",
        "        return pd.DataFrame(columns=['channel', 'total_msgs', 'ai_msgs', 'ai_share', 'avg_sentiment', 'avg_reactions'])\n",
        "\n",
        "    # total msgs per channel (all messages you pulled from Mongo)\n",
        "    base = df_all.groupby(\"channel\")[\"text\"].count().rename(\"total_msgs\").to_frame()\n",
        "    # ai messages per channel\n",
        "    ai_cnt = df_ai_enriched.groupby(\"channel\")[\"text\"].count().rename(\"ai_msgs\").to_frame()\n",
        "    out = base.join(ai_cnt, how=\"left\").fillna(0)\n",
        "    out[\"ai_msgs\"] = out[\"ai_msgs\"].astype(int)\n",
        "    out[\"ai_share\"] = (out[\"ai_msgs\"] / out[\"total_msgs\"]).round(3)\n",
        "    # avg sentiment & reactions for AI messages\n",
        "    avg_sent = df_ai_enriched.groupby(\"channel\")[\"sentiment_score\"].mean().rename(\"avg_sentiment\")\n",
        "    avg_re = df_ai_enriched.groupby(\"channel\")[\"reaction_count\"].mean().rename(\"avg_reactions\")\n",
        "    out = out.join(avg_sent, how=\"left\").join(avg_re, how=\"left\")\n",
        "    return out.reset_index().sort_values(\"ai_msgs\", ascending=False)\n",
        "\n",
