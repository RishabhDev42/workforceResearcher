      "cell_type": "code",
      "source": [
        "from sklearn.feature_extraction.text import TfidfVectorizer\n",
        "from sklearn.decomposition import NMF\n",
        "\n",
        "TOP_K_TERMS = 25     # export this many keywords\n",
        "N_TOPICS    = 5      # number of topics\n",
        "TOP_TERMS   = 8      # terms to show per topic\n",
        "\n",
        "def tfidf_top_terms(texts, top_k=25):\n",
        "    vec = TfidfVectorizer(stop_words=\"english\", max_df=0.8, min_df=2, ngram_range=(1,2))\n",
        "    X = vec.fit_transform(texts)\n",
        "    vocab = vec.get_feature_names_out()\n",
        "    scores = X.sum(axis=0).A1\n",
        "    order = scores.argsort()[::-1][:top_k]\n",
        "    return [(vocab[i], float(scores[i])) for i in order], vec, X\n",
        "\n",
        "def nmf_topics(X, vocab, n_topics=5, top_terms=8):\n",
        "    nmf = NMF(n_components=n_topics, init=\"nndsvd\", random_state=42, max_iter=400)\n",
        "    W = nmf.fit_transform(X)\n",
        "    H = nmf.components_\n",
        "    topics = []\n",
        "    for k in range(n_topics):\n",
        "        top_idx = H[k].argsort()[::-1][:top_terms]\n",
        "        topics.append({\"topic_id\": k, \"terms\": [vocab[i] for i in top_idx]})\n",
        "    doc_topics = W.argmax(axis=1)\n",
        "    return topics, doc_topics\n",
        "\n",
        "kw_pairs, vec, X = tfidf_top_terms(df_ai[\"clean_text\"].tolist(), top_k=TOP_K_TERMS)\n",
        "vocab = vec.get_feature_names_out()\n",
        "topics, doc_topics = nmf_topics(X, vocab, n_topics=N_TOPICS, top_terms=TOP_TERMS)\n",
        "df_ai[\"topic_id\"] = doc_topics\n",
        "\n",
        "print(\"Top keywords:\")\n",
        "pd.DataFrame(kw_pairs, columns=[\"term\",\"tfidf_sum\"]).head(10)\n"
      ],
