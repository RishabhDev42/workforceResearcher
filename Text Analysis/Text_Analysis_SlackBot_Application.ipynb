      "cell_type": "code",
      "source": [
        "import re\n",
        "from openai import OpenAI\n",
        "from google.colab import userdata # Import userdata\n",
        "\n",
        "# Get API key from Colab secrets\n",
        "openai_api_key = userdata.get('OPENAI_API_KEY')\n",
        "\n",
        "# Initialize OpenAI client\n",
        "client = OpenAI(api_key=openai_api_key)\n",
        "\n",
        "# Pre-compile keyword regex\n",
        "AI_PATTERN = re.compile(\"|\".join(AI_KEYWORDS), flags=re.IGNORECASE)\n",
        "\n",
        "def keyword_mask(texts: pd.Series) -> np.ndarray:\n",
        "    return texts.astype(str).str.contains(AI_PATTERN, na=False).values\n",
        "\n",
        "# Efficient cosine sim on matrices\n",
        "def cosine_sim_matrix(A, B):\n",
        "    A = A / np.linalg.norm(A, axis=1, keepdims=True)\n",
        "    B = B / np.linalg.norm(B, axis=1, keepdims=True)\n",
        "    return A @ B.T\n",
        "\n",
        "def embed_batch(strings):\n",
        "    resp = client.embeddings.create(model=EMBED_MODEL, input=strings)\n",
        "    return np.array([d.embedding for d in resp.data], dtype=np.float32)\n",
        "\n",
        "# Precompute topic embeddings once\n",
        "TOPIC_EMB = embed_batch(REFERENCE_TOPICS)\n",
        "\n",
        "def semantic_mask(texts: pd.Series, threshold=SIM_THRESHOLD, batch_size=BATCH_SIZE) -> np.ndarray:\n",
        "    idxs = np.arange(len(texts))\n",
        "    mask = np.zeros(len(texts), dtype=bool)\n",
        "    for i in range(0, len(texts), batch_size):\n",
        "        chunk = texts.iloc[i:i+batch_size].astype(str).tolist()\n",
        "        emb = embed_batch(chunk)                           # (n, d)\n",
        "        sims = cosine_sim_matrix(emb, TOPIC_EMB)          # (n, k)\n",
        "        max_sim = sims.max(axis=1)\n",
        "        mask[i:i+batch_size] = max_sim >= threshold\n",
        "    return mask\n",
        "\n",
        "def explain_match(row_text):\n",
        "    # small helper for debugging\n",
        "    m = AI_PATTERN.search(row_text or \"\")\n",
        "    return \"keyword\" if m else \"semantic\""
      ],
