        "def top_examples(df, label, k=3, by=\"sentiment_score\"):\n",
        "    if label == \"positive\":\n",
        "        return df[df[\"sentiment_label\"]==label].nlargest(k, by)[[\"text\",\"sentiment_score\"]]\n",
        "    if label == \"negative\":\n",
        "        return df[df[\"sentiment_label\"]==label].nsmallest(k, by)[[\"text\",\"sentiment_score\"]]\n",
        "    return df[df[\"sentiment_label\"]==label].head(k)[[\"text\",\"sentiment_score\"]]\n",
        "\n",
        "ex_pos = top_examples(df_ai, \"positive\", 3)\n",
        "ex_neg = top_examples(df_ai, \"negative\", 3)\n",
        "ex_neu = top_examples(df_ai, \"neutral\", 3)\n",
