        "from collections import Counter\n",
        "\n",
        "def word_frequencies(texts, stopwords=custom_stop, top_k=100):\n",
        "    tokens = []\n",
        "    for t in texts:\n",
        "        if isinstance(t,str):\n",
        "            tokens.extend([w for w in t.split() if w not in stopwords])\n",
        "    counts = Counter(tokens)\n",
        "    return pd.DataFrame(counts.most_common(top_k), columns=[\"word\",\"count\"])\n",
        "\n",
        "# Overall\n",
        "wc_all = word_frequencies(df_ai[\"clean_text\"].tolist())\n",
        "wc_all.to_csv(f\"{OUTPUT_DIR}/wordcloud_all.csv\", index=False)\n",
        "\n",
        "# Per sentiment\n",
        "for lab in [\"positive\",\"neutral\",\"negative\"]:\n",
        "    subset = df_ai[df_ai[\"sentiment_label\"]==lab][\"clean_text\"].tolist()\n",
        "    wc_df = word_frequencies(subset)\n",
        "    wc_df.to_csv(f\"{OUTPUT_DIR}/wordcloud_{lab}.csv\", index=False)\n",
        "\n",
        "print(\"Saved WordCloud word-frequency CSVs to:\", OUTPUT_DIR)\n"
