        "def build_wordcloud(texts, title, outpath):\n",
        "    txt = \" \".join([t for t in texts if isinstance(t,str)])\n",
        "    if not txt.strip():\n",
        "        print(f\"[skip] No text for {title}\")\n",
        "        return\n",
        "    wc = WordCloud(width=1200, height=600, stopwords=custom_stop, background_color=\"white\").generate(txt)\n",
        "    plt.figure(figsize=(12,6))\n",
        "    plt.imshow(wc, interpolation=\"bilinear\"); plt.axis(\"off\"); plt.title(title)\n",
        "    plt.tight_layout(); plt.savefig(outpath); plt.close()\n",
        "\n",
        "# Overall\n",
        "build_wordcloud(df_ai[\"clean_text\"].tolist(), \"Word Cloud â€“ All AI Messages\", f\"{OUTPUT_DIR}/wc_all.png\")\n",
