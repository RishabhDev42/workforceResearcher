    {
      "cell_type": "code",
      "source": [
        "texts = df_raw[\"text\"]\n",
        "kw = keyword_mask(texts)             # boolean array\n",
        "print(\"Keyword matches:\", kw.sum())\n",
        "\n",
        "# Only compute embeddings for those NOT matched by keywords (saves tokens/cost)\n",
        "to_embed_idx = np.where(~kw)[0]\n",
        "sem = np.zeros(len(texts), dtype=bool)\n",
        "if len(to_embed_idx) > 0:\n",
        "    sem_sub = semantic_mask(texts.iloc[to_embed_idx])\n",
        "    sem[to_embed_idx] = sem_sub\n",
        "print(\"Semantic-only matches:\", sem.sum())\n",
        "\n",
        "hybrid = kw | sem\n",
        "print(\"Total AI-like messages (hybrid):\", hybrid.sum(), \"of\", len(texts))\n",
        "\n",
        "df_ai = df_raw.loc[hybrid].copy()\n",
        "df_ai[\"ai_match_type\"] = np.where(kw[hybrid], \"keyword\",\n",
        "                           np.where(sem[hybrid], \"semantic\", \"unknown\"))\n",
        "df_ai.head(10)\n"
