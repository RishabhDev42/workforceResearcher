      "cell_type": "code",
      "source": [
        "def mongo_to_df(uri, db, coll, start_iso=None, end_iso=None):\n",
        "    client = MongoClient(uri, retryWrites=True)\n",
        "    c = client[db][coll]\n",
        "    q = {}\n",
        "    if start_iso or end_iso:\n",
        "        q[\"created_at\"] = {}\n",
        "        if start_iso: q[\"created_at\"][\"$gte\"] = start_iso\n",
        "        if end_iso:   q[\"created_at\"][\"$lte\"] = end_iso\n",
        "    proj = {\"_id\":0, \"team_id\":1, \"channel\":1, \"ts\":1, \"user\":1,\n",
        "            \"text\":1, \"thread_ts\":1, \"reactions\":1, \"created_at\":1}\n",
        "    docs = list(c.find(q, proj))\n",
        "    df = pd.DataFrame(docs)\n",
        "\n",
        "    # Ensure 'text' column exists\n",
        "    if \"text\" not in df.columns:\n",
        "        for cand in [\"message\", \"message_text\", \"body\", \"content\"]:\n",
        "            if cand in df.columns:\n",
        "                df = df.rename(columns={cand: \"text\"})\n",
        "                break\n",
        "        if \"text\" not in df.columns:\n",
        "             print(\"Warning: No text column found.\") # or raise error\n",
        "             df[\"text\"] = \"\" # Add an empty text column to prevent downstream errors\n",
        "\n",
        "    # Ensure 'created_at' column exists, derive from ts if needed\n",
        "    if \"created_at\" not in df.columns:\n",
        "        if \"ts\" in df.columns:\n",
        "            def ts_to_dt(ts):\n",
        "                try:\n",
        "                    sec = float(str(ts).split(\".\")[0])\n",
        "                    return datetime.fromtimestamp(sec, tz=timezone.utc)\n",
        "                except Exception:\n",
        "                    return pd.NaT\n",
        "            df[\"created_at\"] = df[\"ts\"].apply(ts_to_dt)\n",
        "        else:\n",
        "            print(\"Warning: No created_at or ts column found.\")\n",
        "            df[\"created_at\"] = pd.NaT # Add a column with NaT if neither is found\n",
        "\n",
        "    # Convert to datetime if 'created_at' column exists (which it now always should)\n",
        "    if \"created_at\" in df.columns:\n",
        "        df[\"created_at\"] = pd.to_datetime(df[\"created_at\"], errors=\"coerce\", utc=True)\n",
        "    return df\n",
        "\n",
        "df_raw = mongo_to_df(MONGO_URI, MONGO_DB, MONGO_COLL, DATE_START, DATE_END).dropna(subset=[\"text\"])\n",
        "print(\"Raw shape:\", df_raw.shape)\n",
        "df_raw.head(3)"
      ],
