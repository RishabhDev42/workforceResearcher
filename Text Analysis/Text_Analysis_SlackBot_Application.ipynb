        "URL_RE   = re.compile(r\"https?://\\S+|www\\.\\S+\")\n",
        "MENTION  = re.compile(r\"<@[\\w\\d]+>\")\n",
        "NONALNUM = re.compile(r\"[^a-z0-9\\s']\")\n",
        "\n",
        "def clean_text(s: str) -> str:\n",
        "    s = str(s).lower()\n",
        "    s = URL_RE.sub(\" \", s)\n",
        "    s = MENTION.sub(\" \", s)\n",
        "    s = NONALNUM.sub(\" \", s)\n",
        "    return re.sub(r\"\\s+\", \" \", s).strip()\n",
        "\n",
        "df_ai[\"clean_text\"] = df_ai[\"text\"].fillna(\"\").apply(clean_text)\n",
        "print(df_ai.shape)\n",
        "df_ai[[\"text\",\"ai_match_type\",\"clean_text\"]].head(10)\n"
