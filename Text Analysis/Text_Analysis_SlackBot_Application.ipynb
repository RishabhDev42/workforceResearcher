        "from vaderSentiment.vaderSentiment import SentimentIntensityAnalyzer\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "\n",
        "analyzer = SentimentIntensityAnalyzer()\n",
        "\n",
        "def add_sentiment(df):\n",
        "    df = df.copy()\n",
        "    scores, labels = [], []\n",
        "    for t in df[\"clean_text\"].tolist():\n",
        "        vs = analyzer.polarity_scores(t or \"\")\n",
        "        c = vs[\"compound\"]\n",
        "        scores.append(c)\n",
        "        labels.append(\"positive\" if c >= 0.05 else \"negative\" if c <= -0.05 else \"neutral\")\n",
        "    df[\"sentiment_score\"] = scores\n",
        "    df[\"sentiment_label\"] = labels\n",
        "    return df\n",
        "\n",
        "def add_engagement(df):\n",
        "    def react_count(rlist):\n",
        "        if not isinstance(rlist, list): return 0\n",
        "        return int(sum((r.get(\"count\",0) or 0) for r in rlist))\n",
        "    df = df.copy()\n",
        "    df[\"reaction_count\"] = df.get(\"reactions\", 0)\n",
        "    df[\"reaction_count\"] = df[\"reaction_count\"].apply(react_count)\n",
        "    return df\n",
        "\n",
        "df_ai = add_engagement(add_sentiment(df_ai))\n",
        "df_ai[[\"text\",\"sentiment_label\",\"sentiment_score\",\"reaction_count\"]].head(50)\n"
      ],
